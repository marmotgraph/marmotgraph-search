{
  "@context": {
    "@vocab": "https://core.kg.ebrains.eu/vocab/query/",
    "query": "https://schema.hbp.eu/myQuery/",
    "propertyName": {
      "@id": "propertyName",
      "@type": "@id"
    },
    "merge": {
      "@type": "@id",
      "@id": "merge"
    },
    "path": {
      "@id": "path",
      "@type": "@id"
    }
  },
  "meta": {
    "name": "Subjects search query",
    "type": "https://openminds.om-i.org/types/Subject",
    "responseVocab": "https://schema.hbp.eu/myQuery/"
  },
  "structure": [
    {
      "propertyName": "query:identifier",
      "path": "http://schema.org/identifier",
      "required": true
    },
    {
      "propertyName": "query:id",
      "path": "@id",
      "required": true
    },
    {
      "propertyName": "query:type",
      "path": "@type"
    },
    {
      "propertyName": "query:title",
      "path": "https://openminds.om-i.org/props/lookupLabel"
    },
    {
      "propertyName": "query:species",
      "path": [
        "https://openminds.om-i.org/props/species",
        "https://openminds.om-i.org/props/name"
      ]
    },
    {
      "propertyName": "query:sex",
      "path": [
        "https://openminds.om-i.org/props/biologicalSex",
        "https://openminds.om-i.org/props/name"
      ]
    },
    {
      "propertyName": "query:studiedState",
      "path": "https://openminds.om-i.org/props/studiedState",
      "structure": [
        {
          "propertyName": "query:age",
          "path": [
            "https://openminds.om-i.org/props/age",
            "https://openminds.om-i.org/props/value"
          ],
          "singleValue": "FIRST"
        },
        {
          "propertyName": "query:ageCategory",
          "path": [
            "https://openminds.om-i.org/props/ageCategory",
            "https://openminds.om-i.org/props/name"
          ]
        },
        {
          "propertyName": "query:weight",
          "path": [
            "https://openminds.om-i.org/props/weight",
            "https://openminds.om-i.org/props/value"
          ],
          "singleValue": "FIRST"
        }
      ]
    },
    {
      "propertyName": "query:strain",
      "path": [
        "https://openminds.om-i.org/props/strain",
        "https://openminds.om-i.org/props/name"
      ],
      "singleValue": "FIRST"
    },
    {
      "propertyName": "query:genotype",
      "path": [
        "https://openminds.om-i.org/props/genotype",
        "https://openminds.om-i.org/props/name"
      ],
      "singleValue": "FIRST"
    },
    {
      "propertyName": "query:datasets",
      "path": {
        "@id": "https://openminds.om-i.org/props/studiedSpecimen",
        "reverse": true
      },
      "structure": [
        {
          "propertyName": "query:id",
          "path": "@id",
          "required": true
        },
        {
          "propertyName": "query:fullName",
          "path": "https://openminds.om-i.org/props/fullName"
        },
        {
          "propertyName": "query:componentName",
          "path": [
            {
              "@id": "https://openminds.om-i.org/props/hasPart",
              "reverse": true
            },
            "https://openminds.om-i.org/props/fullName"
          ]
        }
      ]
    }
  ]
}